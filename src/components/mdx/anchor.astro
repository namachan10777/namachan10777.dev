---
export interface Props {
  href: string;
  label?: string;
}

const { href, label } = Astro.props;
---

{
  href.startsWith("http") ? (
    <a href={href} aria-label={label} class="text-emerald-700 underline">
      <slot />
    </a>
  ) : (
    <a
      href={href}
      aria-label={label}
      class="text-emerald-700 underline"
      data-astro-prefetch
    >
      <slot />
    </a>
  )
}
