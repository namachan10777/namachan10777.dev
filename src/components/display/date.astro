---
export type Props = {
  date: Date;
};
function dateToString(date: Date): string {
  const year = `0${date.getFullYear()}`.slice(-4);
  const month = `0${date.getMonth() + 1}`.slice(-2);
  const day = `0${date.getDate()}`.slice(-2);
  return `${year}-${month}-${day}`;
}
---

{dateToString(Astro.props.date)}
