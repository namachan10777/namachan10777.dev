{
  "name": "namachan10777-dev",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "build.astro": "bunx --bun astro build",
    "build.pagefind": "bunx --bun pagefind --site dist",
    "build.pagefind-dev": "bunx --bun pagefind --site dist --output-path public/pagefind",
    "build": "run-s build.astro build.pagefind",
    "dev.prepare": "run-s build.astro build.pagefind-dev",
    "dev": "bunx --bun astro dev",
    "preview": "bunx --bun astro preview",
    "astro": "astro",
    "lint.astro": "bunx --bun astro check",
    "lint.eslint": "eslint .",
    "lint.fmt": "prettier --check .",
    "lint.stylelint": "stylelint '**/*.{astro,css}'",
    "lint": "run-p lint.*",
    "fix.eslint": "eslint --fix .",
    "fix.fmt": "prettier --write .",
    "fix.stylelint": "stylelint --fix '**/*.{astro,css}'",
    "fix": "run-s fix.*"
  },
  "dependencies": {
    "@astrojs/check": "^0.7.0",
    "@astrojs/mdx": "^3.1.2",
    "@astrojs/rss": "^4.0.7",
    "@astrojs/sitemap": "^3.1.6",
    "@iconify-json/iconoir": "^1.1.44",
    "@types/bun": "^1.1.6",
    "@types/license-checker": "^25.0.6",
    "@types/remark-heading-id": "^1.0.0",
    "@types/svg-parser": "^2.0.6",
    "astro": "^4.11.4",
    "astro-icon": "^1.1.0",
    "astro-seo": "^0.8.4",
    "hast-util-to-html": "^9.0.1",
    "license-checker": "^25.0.1",
    "license-checker-rseidelsohn": "^4.3.0",
    "lightningcss": "^1.25.1",
    "ogp-image": "workspace:*",
    "pagefind": "^1.1.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-katex": "^7.0.0",
    "rehype-pretty-code": "^0.13.2",
    "rehype-slug": "^6.0.0",
    "remark-custom-heading-id": "^2.0.0",
    "remark-gemoji": "^8.0.0",
    "remark-gfm": "^4.0.0",
    "remark-heading-id": "^1.0.1",
    "remark-math": "^6.0.0",
    "remark-retext": "^6.0.0",
    "remark-sectionize": "^2.0.0",
    "remark-stringify": "^11.0.0",
    "retext-english": "^5.0.0",
    "retext-equality": "^7.1.0",
    "retext-stringify": "^4.0.0",
    "sharp": "^0.33.4",
    "shiki": "^1.10.1",
    "shiki-copy-button": "workspace:*",
    "svg-parser": "^2.0.4",
    "typescript": "^5.5.3",
    "unified": "^11.0.5",
    "vite-tsconfig-paths": "^4.3.2"
