{
  "name": "my-qwik-basic-starter",
  "description": "Demo App with Routing built-in (recommended)",
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "trustedDependencies-annotation": "Needed for bun to allow running install scripts",
  "type": "module",
  "scripts": {
    "build": "content-collections build && qwik build && pagefind --site dist --output-path dist/pagefind",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/static/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "build.pagefind": "pagefind --site dist --output-path dist/pagefind",
    "prepare.pagefind": "pagefind --site dist --output-path public/pagefind",
    "build.collections": "content-collections build",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint .",
    "lint.fix": "eslint --fix .",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.5.4",
    "@builder.io/qwik-city": "^1.5.4",
    "@content-collections/cli": "^0.1.2",
    "@content-collections/core": "^0.4.1",
    "@content-collections/vite": "^0.2.1",
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^9.2.0",
    "@qwik-ui/headless": "^0.3.8",
    "@qwikest/icons": "^0.0.13",
    "@types/eslint": "^8.56.10",
    "@types/hast": "^3.0.4",
    "@types/mdast": "^4.0.4",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.2",
    "@types/xml2js": "^0.4.14",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-qwik": "^1.5.4",
    "node-fetch-cache": "^4.1.1",
    "pagefind": "^1.1.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "react": "^18.3.1",
    "remark-gfm": "^4.0.0",
    "remark-mdx": "^3.0.1",
    "remark-parse": "^11.0.0",
    "satori": "^0.10.13",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.5",
    "typescript-eslint": "^7.8.0",
    "undici": "^6.16.1",
    "unified": "^11.0.4",
    "vite": "^5.2.11",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "dependencies": {
    "rehype": "^13.0.1",
    "remark": "^15.0.1",
    "remark-retext": "^6.0.0",
    "remark-sectionize": "^2.0.0",
    "remark-stringify": "^11.0.0",
    "retext-english": "^5.0.0",
    "retext-equality": "^7.1.0",
    "sharp": "^0.33.3",
    "shiki": "^1.5.2",
    "xml2js": "^0.6.2"
  }
}
